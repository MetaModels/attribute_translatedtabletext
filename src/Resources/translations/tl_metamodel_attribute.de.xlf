<?xml version="1.0" ?><xliff xmlns="urn:oasis:names:tc:xliff:document:1.2" version="1.2">
  <file source-language="en" datatype="plaintext" original="src/Resources/translations/tl_metamodel_attribute.en.xlf" target-language="de">
    <body>
      <trans-unit id="typeOptions.translatedtabletext" resname="typeOptions.translatedtabletext">
        <source>Translated table text</source>
        <target>Übersetzte Text-Tabelle</target>
      </trans-unit>
      <trans-unit id="translatedtabletext_cols.label" resname="translatedtabletext_cols.label">
        <source>Column config</source>
        <target>Spalteneinstellung</target>
      </trans-unit>
      <trans-unit id="translatedtabletext_cols.description" resname="translatedtabletext_cols.description">
        <source>Please enter a label and width for each column.</source>
        <target>Bitte ein Label und die Breite für jede Spalte angeben.</target>
      </trans-unit>
      <trans-unit id="translatedtabletext_langcode.label" resname="translatedtabletext_langcode.label">
        <source>Language code</source>
        <target>Sprachencode</target>
      </trans-unit>
      <trans-unit id="translatedtabletext_langcode.description" resname="translatedtabletext_langcode.description">
        <source>The language code for the column.</source>
        <target>Sprachencode für die Spalten.</target>
      </trans-unit>
      <trans-unit id="translatedtabletext_rowLabel.label" resname="translatedtabletext_rowLabel.label">
        <source>Label</source>
        <target>Label</target>
      </trans-unit>
      <trans-unit id="translatedtabletext_rowLabel.description" resname="translatedtabletext_rowLabel.description">
        <source>Please enter the label for the column.</source>
        <target>Bitte das Label für die Spalte angeben.</target>
      </trans-unit>
      <trans-unit id="translatedtabletext_rowStyle.label" resname="translatedtabletext_rowStyle.label">
        <source>Width</source>
        <target>Breite</target>
      </trans-unit>
      <trans-unit id="translatedtabletext_rowStyle.description" resname="translatedtabletext_rowStyle.description">
        <source>Please enter the width of the column (e.g. 200px).</source>
        <target>Bitte geben Sie die Spaltenbreite ein (z. B. 200px).</target>
      </trans-unit>
      <trans-unit id="translatedtabletext_minCount.label" resname="translatedtabletext_minCount.label">
        <source>Minimum count of rows</source>
        <target>Mindestanzahl der Zeilen</target>
      </trans-unit>
      <trans-unit id="translatedtabletext_minCount.description" resname="translatedtabletext_minCount.description">
        <source>Please enter minimum count of rows - 0 sets the value without limit.</source>
        <target>Bitte geben Sie die Mindestanzahl der Zeilen ein - 0 setzt den Wert ohne Begrenzung.</target>
      </trans-unit>
      <trans-unit id="translatedtabletext_maxCount.label" resname="translatedtabletext_maxCount.label">
        <source>Maximum count of rows</source>
        <target>Maximale Anzahl von Zeilen</target>
      </trans-unit>
      <trans-unit id="translatedtabletext_maxCount.description" resname="translatedtabletext_maxCount.description">
        <source>Please enter maximum count of rows - 0 sets the value without limit.</source>
        <target>Bitte geben Sie die maximale Anzahl der Zeilen ein - 0 setzt den Wert ohne Begrenzung.</target>
      </trans-unit>
      <trans-unit id="translatedtabletext_disable_sorting.label" resname="translatedtabletext_disable_sorting.label">
        <source>Disable sorting</source>
        <target>Sortierung deaktivieren</target>
      </trans-unit>
      <trans-unit id="translatedtabletext_disable_sorting.description" resname="translatedtabletext_disable_sorting.description">
        <source>If this option is activated, the sorting is hidden.</source>
        <target>Wenn diese Option aktiviert ist, wird die Sortierung ausgeblendet.</target>
      </trans-unit>
      <trans-unit id="translatedtabletext_quantity_cols.label" resname="translatedtabletext_quantity_cols.label">
        <source>Number of columns</source>
        <target>Anzahl der Spalten</target>
      </trans-unit>
      <trans-unit id="translatedtabletext_quantity_cols.description" resname="translatedtabletext_quantity_cols.description">
        <source>Please select the number of columns.</source>
        <target>Bitte wählen Sie die Anzahl der Spalten aus.</target>
      </trans-unit>
      <trans-unit id="translatedtabletext_rowLabels.label" resname="translatedtabletext_rowLabels.label">
        <source>Configure columns</source>
        <target>Spalteneinstellung</target>
      </trans-unit>
      <trans-unit id="translatedtabletext_rowLabels.description" resname="translatedtabletext_rowLabels.description">
        <source>Configure each colum with label and width.</source>
        <target>Bitte ein Label und die Breite für jede Spalte angeben.</target>
      </trans-unit>
    </body>
  </file>
</xliff>